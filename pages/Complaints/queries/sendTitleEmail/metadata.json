{
  "gitSyncId": "66fb995faf2b8e5a2a63f337_12dfcca7-b1a7-48b8-84d7-fd0adb3e81d9",
  "id": "Complaints_sendTitleEmail",
  "pluginId": "smtp-plugin",
  "pluginType": "DB",
  "unpublishedAction": {
    "actionConfiguration": {
      "body": "{{AddTitles.generateEmailTitleTemplate()}}",
      "encodeParamsToggle": true,
      "formData": {
        "command": "SEND",
        "send": {
          "attachments": "{{ListTitles.titleList.map(item=>item.FilePicker2[0])}}",
          "bodyType": "text/html",
          "from": "saip@naxap.com",
          "subject": "Request for Verification of the Titles of {{appsmith.store.signUpRightHolderName}} Regarding the IP Violation Complaint",
          "to": "{{appsmith.store.signUpRightHolderEmail}}"
        }
      },
      "paginationType": "NONE",
      "timeoutInMillisecond": 10000
    },
    "confirmBeforeExecute": false,
    "datasource": {
      "id": "GMAIL",
      "isAutoGenerated": false,
      "name": "GMAIL",
      "pluginId": "smtp-plugin"
    },
    "dynamicBindingPathList": [
      {
        "key": "formData.send.subject"
      },
      {
        "key": "formData.send.to"
      },
      {
        "key": "body"
      },
      {
        "key": "formData.send.attachments"
      }
    ],
    "executeOnLoad": false,
    "name": "sendTitleEmail",
    "pageId": "Complaints",
    "userSetOnLoad": false
  }
}